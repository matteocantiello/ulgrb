ticksize -1 0 -1 0
data mdot25.dat
read t 1 
read mdot 2
read mdot2 4
set tt=lg(t)
set mm=lg(mdot+1.e-8)
set mm2=lg(mdot2+1.e-8)
limits -2 4.8 -7 1.7
ctype black
lw 1
box
lw 3
ltype 0
connect tt mm
ltype 2
ctype blue
connect tt mm2
ctype 0
relocate -1.5 -3
ctype 0
ltype 0
draw 0 -3
relocate 0.1 -3
expand 1.8
lw 1
ctype black
label \dot{m}_{fb}
ctype blue
ltype 2
relocate -1.5 -3.8
lw 3
draw 0 -3.8
relocate 0.1 -3.8
expand 1.8
lw 1
ctype black
label \dot{m_{fb,disk}}
